#lang s-exp "project-datum-v1.rkt"

(require rackunit)


(check-equal? (intersection (list (cons 2 4) (cons 3 7)) (list (cons 2 1) (cons 3 2) (cons 7 1)))
              '((2 . 1) (3 . 2)))
(check-equal? (intersection ((2 ^ 4) (3 ^ 7)) ((2 ^ 1) (3 ^ 2) (7 ^ 1)))
              '((2 . 1) (3 . 2)))
(check-equal? (∩ ((2 ^ 4) (3 ^ 7)) ((2 ^ 1) (3 ^ 2) (7 ^ 1)))
              '((2 . 1) (3 . 2)))

(check-equal? (union ((2 ^ 4) (3 ^ 5)) ((7 ^ 3)))
              '((2 . 4) (3 . 5) (7 . 3)))
(check-equal? (∪ ((2 ^ 4) (3 ^ 5)) ((7 ^ 3)))
              '((2 . 4) (3 . 5) (7 . 3)))
(check-equal? (union ((7 ^ 3)) ((2 ^ 4) (3 ^ 5)))
              '((2 . 4) (3 . 5) (7 . 3)))
(check-equal? (∪ ((7 ^ 3)) ((2 ^ 4) (3 ^ 5)))
              '((2 . 4) (3 . 5) (7 . 3)))